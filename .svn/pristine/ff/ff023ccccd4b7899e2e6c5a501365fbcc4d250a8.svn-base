<!DOCTYPE html>
<html>
    <head>
        <title>setInterval</title>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width">
    </head>
    <body>
        <script>
            setInterval(function(){
                var date = new Date();
                console.log(date);
            }, 2000);
        </script>
    </body>
</html>
